{
  "name": "data-viz-d3-and-firebase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:ci": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --no-fix --ignore-path .gitignore",
    "dev:e2e": "vite --mode e2e",
    "setup": "./setup/project_setup.sh",
    "size": "npm run build && size-limit",
    "test:unit:ci": "npm run test:unit -- --ci --coverage",
    "utils:gen-preview": "npx cypress run --config-file cypress.doc.json --spec 'cypress/documentation/documentation.spec.js' && node ./ci/export-documentation.js"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "d3": "^5.11.0",
    "d3-svg-legend": "^2.25.6",
    "d3-tip": "^0.9.1",
    "firebase": "^9.15.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "process": "^0.11.10",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@size-limit/file": "~8.2.4",
    "@size-limit/webpack": "~8.2.4",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vue/cli-plugin-unit-jest": "^5.0.8",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/test-utils": "1.3.5",
    "@vue/vue2-jest": "^27.0.0",
    "cypress": "^9.4.1",
    "eslint": "~8.31",
    "eslint-plugin-vue": "^9.10.0",
    "looks-same": "^7.3.0",
    "sass": "^1.62.0",
    "size-limit": "~8.2.4",
    "vite": "^4.2.1"
  },
  "engines": {
    "node": ">= 18",
    "npm": ">= 9"
  }
}
